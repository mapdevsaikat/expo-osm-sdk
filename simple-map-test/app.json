{
  "expo": {
    "name": "Expo OSM SDK Showcase",
    "slug": "expo-osm-sdk-showcase",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.test.simplemap",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to show your position on the map and for navigation features.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to show your position on the map and for navigation features."
      }
    },
    "android": {
      "package": "com.test.simplemap",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "platforms": ["ios", "android", "web"],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
    ],
    "extra": {
      "eas": {
        "projectId": "90a12594-3647-4c8c-8136-9105992ee3df"
      }
    }
  }
}
