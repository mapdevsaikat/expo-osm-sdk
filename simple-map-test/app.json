{
  "expo": {
    "name": "Simple Map Test",
    "slug": "simple-map-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.test.simplemap"
    },
    "android": {
      "package": "com.test.simplemap",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      "expo-osm-sdk/plugin"
    ],
    "extra": {
      "eas": {
        "projectId": "simple-map-test-osm"
      }
    }
  }
} 