{
  "expo": {
    "name": "OSM Expo SDK Demo",
    "slug": "osm-expo-sdk-demo",
    "version": "1.0.0",
    "owner": "mapdevsaikat",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "osm-expo-sdk-demo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mapdevsaikat.osmexposdkdemo"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser",
      [
        "expo-osm-sdk/plugin",
        {
          "locationPermissionText": "This app uses location for map features"
        }
      ]
    ],
    "android": {
      "package": "com.mapdevsaikat.osmexposdkdemo"
    },
    "extra": {
      "eas": {
        "projectId": "a6a87524-670b-4a55-b512-662cd28915bf"
      }
    }
  }
}
