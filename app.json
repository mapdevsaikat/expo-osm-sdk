{
  "expo": {
    "name": "osm-expo-sdk-test",
    "slug": "osm-expo-sdk-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      ["expo-osm-sdk", { "locationPermissionText": "This app uses location for map features" }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
} 